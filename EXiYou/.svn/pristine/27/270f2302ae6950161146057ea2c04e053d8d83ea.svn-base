package jcp.com.exiyou.ui.community.bean.recommend;

import java.util.List;

/**
 * Created by SweetDream on 2017/3/25.
 */
public class RecommendIndex {

    private int id;
    private String title;
    private List<String> bigpic_arr;
    private String author;
    private String avatar;
    private String forum;
    private int reply_num;
    private String reply_time;
    private String appview_url;
    private int is_best;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public List<String> getBigpic_arr() {
        return bigpic_arr;
    }

    public void setBigpic_arr(List<String> bigpic_arr) {
        this.bigpic_arr = bigpic_arr;
    }

    public String getAuthor() {
        return author;
    }

    public void setAuthor(String author) {
        this.author = author;
    }

    public String getAvatar() {
        return avatar;
    }

    public void setAvatar(String avatar) {
        this.avatar = avatar;
    }

    public String getForum() {
        return forum;
    }

    public void setForum(String forum) {
        this.forum = forum;
    }

    public int getReply_num() {
        return reply_num;
    }

    public void setReply_num(int reply_num) {
        this.reply_num = reply_num;
    }

    public String getReply_time() {
        return reply_time;
    }

    public void setReply_time(String reply_time) {
        this.reply_time = reply_time;
    }

    public String getAppview_url() {
        return appview_url;
    }

    public void setAppview_url(String appview_url) {
        this.appview_url = appview_url;
    }

    public int getIs_best() {
        return is_best;
    }

    public void setIs_best(int is_best) {
        this.is_best = is_best;
    }
}
